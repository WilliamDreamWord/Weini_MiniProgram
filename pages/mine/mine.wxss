/* pages/mine/mine.wxss */
.all {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  width: 100%;
  height: 100%;
  background-color: #F7F7FA; 
}
.row{
  display: flex;
  flex-direction: column;
  width: 100%;
}
.row1 {
  display: flex;
  position: relative;
  flex-direction: column;
  top: -100rpx;
  width: 100%;
  height: auto;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.row open-data {
  position: relative;
  overflow:hidden;
}
#userInfo {
  position: relative;
  overflow:hidden;
  width: 185rpx;
  height:185rpx;
  border-radius:50%;
}
.row2 {
  position: relative;
  top: -250rpx;
  background-color: white;
  width: 100%;
  height: 121rpx;
}
.row2_1 {
  position: relative;
  height: 60rpx;
  font-size: smaller;
  border: 1px solid rgb(236, 236, 236);
}
.row2_1 text{
  display: inline-block;
  line-height: 60rpx;
  position: absolute;
  width: 30%;
  height: 60rpx;
  text-align: center;
}
.row2_1 image{
  display: inline-block;
  position: absolute;
  top: 20%;
  right: 30rpx;
  width: 20rpx;
  height: 30rpx;
}
.row3 {
  position: relative;
  background-color: white;
  width: 100%;
  height: 61rpx;
  top: -190rpx;
}
.row4 {
  position: relative;
  background-color: white;
  width: 100%;
  height: 121rpx;
  top: -130rpx;
}