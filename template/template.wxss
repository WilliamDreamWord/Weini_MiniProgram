/* pages/template/template.wxss */
.icon{
  width:40rpx;
  height: 30rpx;
}
.tabBar{
  width:100%;
  position: fixed;
  bottom:0;
  padding:10rpx;
  margin-left:-4rpx;
  z-index: 10;
  background: white;
  font-size:20rpx;
  color:#8A8A8A;
  box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
}

 .tabBar-item{
  float:left;
  width:33%;
  text-align: center;
  overflow: hidden;
}

/*当前字体颜色*/
.tabBartext{
  color: rgba(202, 137, 17, 0.904);
}